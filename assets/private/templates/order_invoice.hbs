<tr>
	<td style="width:40px">&nbsp;</td>
	<td align="center" style="color: #191716;font-family: 'helvetica neue', 'helvetica',sans-serif;font-size:32px;">{{label_invoice}}</td>
	<td style="width:40px">&nbsp;</td>
</tr>
<tr height="20">
	<td colspan="4"></td>
</tr>
<tr>
	<td align="center" colspan="4">
		<table border="0" cellpadding="0" cellspacing="0" width="660" style="border-collapse:collapse;border-spacing:0">
			<tr>
				<td>
					<table border="0" cellpadding="0" cellspacing="0" style="background-color:#fafafa;border-collapse:collapse;border-spacing:0;color:#191716;font-family:'helvetica neue', 'helvetica',sans-serif;font-size:12px">
						<tbody>
						<tr height="46">
							<td width="320" style="border-color:white;border-style:solid;border-width:0px 1px 1px 0;padding: 20px;vertical-align: top;">
								<span style="color: #191716;font-weight: 500;font-size:10px">{{label_invoice_date}}</span>
								<br>{{INVOICE_DATE}}
							</td>
							<td rowspan="2" width="340" style="border:0px solid white;padding:20px;vertical-align: top;">
								<span style="color: #191716;font-weight: 500;font-size:10px">{{label_address}}</span>
								{{ADDRESS}}
							</td>
						</tr>
						<tr>
							<td width="320" style="padding: 20px;vertical-align: top;border-right: 1px solid #ffffff;">
								<span style="color: #191716;font-weight: 500;font-size:10px">{{label_order_id}}</span>
								<br>{{ORDER_ID}}
							</td>
						</tr>
						</tbody>
					</table>
				</td>
			</tr>
			<tr height="30">
				<td>&nbsp;</td>
			</tr>
			<tr>
				<td>
					<table border="0" cellpadding="0" cellspacing="0" width="660" style="border-collapse:collapse;border-spacing:0;color:rgb(51,51,51);font-family:'helvetica neue','helvetica','arial',sans-serif;font-size:12px;width:660px">
						<tbody>
						<tr height="24" style="background-color:#fafafa">
							<td colspan="3" width="350" style="padding-left:20px;width:350px">
								<span style="font-size:14px;font-weight:500">{{label_order}}</span>
							</td>
						</tr>
						<tr height="15">
							<td colspan="3">&nbsp;</td>
						</tr>

						{{#each ORDERS}}
							<tr style="max-height:114px">
								<td align="center" valign="top" width="64" style="margin:0;padding:0 0 0 20px;width:64px">
									<img height="64" src="{{IMG}}" width="64" style="margin:0;padding:0">
								</td>
								<td style="line-height:15px;padding:0 0 0 20px">
									<span style="font-weight:600">{{ORDER_NAME}}</span>
									<br><span style="color:#666666">{{../label_color}} </span><span style="font-weight:600">{{COLOR}}</span>
									<br><span style="color:#666666">{{../label_size}} </span><span style="font-weight:600">{{SIZE}}</span>
									<br><span style="color:#666666">{{../label_number}} </span><span style="font-weight:600">{{NUMBER}}</span>
									<br>
								</td>
								<td align="right" valign="top" width="100" style="padding:0 20px 0 0;width:100px">
									<table border="0" cellpadding="0" cellspacing="0" style="border-collapse:collapse;border-spacing:0;font-family:inherit;font-size:12px">
										<tbody>
										<tr>
											<td align="right" colspan="3">
												<span style="font-weight:600;white-space:nowrap">{{PRICE}}</span>
											</td>
										</tr>
										</tbody>
									</table>
								</td>
							<tr height="15">
								<td colspan="3">&nbsp;</td>
							</tr>
							</tr>
						{{/each}}


						<tr height="1">
							<td colspan="3" height="1" style="padding:0 10px 0 10px;border-top: 1px solid #e7e7e7">&nbsp;</td>
						</tr>
						</tbody>
					</table>
				</td>
			</tr>
			<tr>
				<td>
					<table border="0" cellpadding="0" cellspacing="0" width="660" style="min-width: 100%;border-collapse:collapse;border-spacing:0;color:#191716;font-family:'helvetica neue', 'helvetica',sans-serif;width:660px">
						<tbody>
						<tr height="15">
							<td colspan="3">&nbsp;</td>
						</tr>
						<tr>
							<td align="right" colspan="2" style="padding:0 20px 0 0;white-space:nowrap">
								<table align="right" border="0" cellpadding="0" cellspacing="0" style="border-collapse:collapse;border-spacing:0;color:#191716;font-family:'helvetica neue', 'helvetica',sans-serif;font-size:12px">
									<tbody>
									<tr>
										<td align="right">
											<span style="color:#191716;white-space:nowrap">{{label_subtotal}}</span>
										</td>
										<td width="10">&nbsp;</td>
										<td align="right">
											<span style="font-weight:600;white-space:nowrap">{{ORDER_AMOUNT}}</span>
										</td>
									</tr>
									<tr>
										<td colspan="2" height="4">&nbsp;</td>
									</tr>
									<tr>
										<td align="right">
											<span style="color:#191716;white-space:nowrap">{{label_delivery}}</span>
										</td>
										<td width="10">&nbsp;</td>
										<td align="right">
											<span style="font-weight:600;white-space:nowrap">{{label_free}}</span>
										</td>
									</tr>
									</tbody>
								</table>
							</td>
						</tr>
						<tr height="15">
							<td colspan="2">&nbsp;</td>
						</tr>
						<tr height="1">
							<td colspan="2" height="1" style="padding:0 10px 0 10px;border-bottom: 1px solid #e7e7e7;">&nbsp;</td>
						</tr>
						<tr height="48">
							<td align="right" style="border-right: 1px solid #e7e7e7;color: #191716;font-size:10px;font-weight:600;padding:0 30px 0 0">{{label_total}}</td>
							<td align="right" width="90" style="font-size:16px;font-weight:600;padding:0 20px 0 0;white-space:nowrap;width:120px">{{ORDER_AMOUNT}}</td>
						</tr>
						<tr height="1">
							<td colspan="2" height="1" style="padding:0 10px 0 10px;border-top: 1px solid #e7e7e7">&nbsp;</td>
						</tr>
						</tbody>
					</table>
				</td>
			</tr>
			</tbody>
		</table>
	</td>
</tr>
<tr style="height:20px">
	<td colspan="4">&nbsp;</td>
</tr>
<tr>
	<td align="center" colspan="4" style="color: #191716;font-family:'helvetica neue', 'helvetica',sans-serif;font-size:12px">
		{{label_account}}
		<a href="https://plabelclothing.com/account" style="color: #007eff;text-decoration:none" target="_blank">
			{{label_visit_account}}
		</a>
		<br>
		<br>
		{{label_shipping}}
		<br>
		<br>
		{{label_support}}
		<a href="https://plabelclothing.com/support" style="color: #007eff;text-decoration:none" target="_blank">
			{{label_visit_support}}
		</a>
	</td>
</tr>
